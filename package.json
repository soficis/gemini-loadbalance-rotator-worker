{
  "name": "gemini-loadbalance-rotator-worker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "import-keys": "node ./scripts/import-keys.js",
  "push-secrets": "node ./scripts/push-oauth-secrets.js",
  "push-secrets:run": "node ./scripts/push-oauth-secrets.js --run",
    "import-keys:wrangler": "node ./scripts/import-keys.js --wrangler",
    "test:auth": "node ./scripts/test-auth-mock.js",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "build": "wrangler deploy --dry-run --outdir=dist",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "docker:build": "docker-compose build",
    "docker:dev": "docker-compose up --build",
    "docker:start": "docker-compose up",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "docker:logs": "docker-compose logs -f gemini-worker",
    "docker:shell": "docker-compose exec gemini-worker sh"
  },
  "dependencies": {
    "hono": "^4.8.9"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250726.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.30.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "wrangler": "^4.24.3"
  }
}
